.................... void TickInit(void) 
.................... { 
....................     setup_timer_0(RTCC_INTERNAL | RTCC_DIV_16); 
*
01DA:  MOVLW  83
01DC:  MOVWF  FD5
....................     set_timer0(TICK_COUNTER); 
01DE:  MOVLW  0B
01E0:  MOVWF  FD7
01E2:  MOVLW  DB
01E4:  MOVWF  FD6
....................  
....................     enable_interrupts(INT_TIMER0); 
01E6:  BSF    FF2.5
....................     enable_interrupts(GLOBAL); 
01E8:  MOVLW  C0
01EA:  IORWF  FF2,F
.................... } 
01EC:  GOTO   095C (RETURN)
