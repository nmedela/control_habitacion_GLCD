.................... /* 
....................  * ARP Task FSM States 
....................  */ 
.................... typedef enum _ARP_STATE 
.................... { 
....................     SM_ARP_IDLE, 
....................     SM_ARP_REPLY 
.................... } ARP_STATE; 
....................  
....................  
.................... /* 
....................  * This ARP task caches one ARP response. 
....................  */ 
.................... static ARP_STATE smARP; 
....................  
.................... static NODE_INFO Cache; 
....................  
....................  
.................... /********************************************************************* 
....................  * Function:        void ARPInit(void) 
....................  * 
....................  * PreCondition:    None 
....................  * 
....................  * Input:           None 
....................  * 
....................  * Output:          ARP Cache is initialized. 
....................  * 
....................  * Side Effects:    None 
....................  * 
....................  * Overview:        None 
....................  * 
....................  * Note:            None 
....................  ********************************************************************/ 
.................... void ARPInit(void) 
.................... { 
....................     smARP = SM_ARP_IDLE; 
*
06C6:  BCF    59.1
....................  
....................     Cache.MACAddr.v[0] = 0xff; 
06C8:  MOVLW  FF
06CA:  MOVLB  1
06CC:  MOVWF  x16
....................     Cache.MACAddr.v[1] = 0xff; 
06CE:  MOVWF  x17
....................     Cache.MACAddr.v[2] = 0xff; 
06D0:  MOVWF  x18
....................     Cache.MACAddr.v[3] = 0xff; 
06D2:  MOVWF  x19
....................     Cache.MACAddr.v[4] = 0xff; 
06D4:  MOVWF  x1A
....................     Cache.MACAddr.v[5] = 0xff; 
06D6:  MOVWF  x1B
....................  
....................     Cache.IPAddr.Val = 0x0; 
06D8:  CLRF   x1F
06DA:  CLRF   x1E
06DC:  CLRF   x1D
06DE:  CLRF   x1C
.................... } 
06E0:  MOVLB  0
06E2:  GOTO   0960 (RETURN)
....................  
....................  
....................  
.................... /********************************************************************* 
....................  * Function:        BOOL ARPProcess(void) 
....................  * 
....................  * PreCondition:    ARP packet is ready in MAC buffer. 
....................  * 
....................  * Input:           None 
....................  * 
....................  * Output:          ARP FSM is executed. 
....................  * 
....................  * Side Effects:    None 
....................  * 
....................  * Overview:        None 
....................  * 
....................  * Note:            None 
....................  ********************************************************************/ 
.................... BOOL ARPProcess(void) 
.................... { 
....................     NODE_INFO remoteNode; 
....................     BYTE opCode; 
....................  
....................     switch(smARP) 
....................     { 
*
1278:  MOVLW  00
127A:  BTFSC  59.1
127C:  MOVLW  01
127E:  XORLW  00
1280:  BZ    1284
1282:  BRA    12E2
....................     case SM_ARP_IDLE: 
....................         if ( !ARPGet(&remoteNode, &opCode) ) 
1284:  MOVLW  01
1286:  MOVLB  1
1288:  MOVWF  x8B
128A:  MOVLW  7F
128C:  MOVWF  x8A
128E:  MOVLW  01
1290:  MOVWF  x8D
1292:  MOVLW  89
1294:  MOVWF  x8C
1296:  MOVLB  0
1298:  BRA    0C42
129A:  MOVF   01,F
129C:  BNZ   12A0
....................             break; 
129E:  BRA    1302
....................  
....................          //dsr add 071204 
....................          //dsr fix 071206 
....................          MACDiscardRx(); 
12A0:  CALL   0966
....................  
....................         if ( opCode == ARP_REPLY ) 
12A4:  MOVLB  1
12A6:  MOVF   x89,F
12A8:  BNZ   12DE
....................         { 
.................... 			Cache.MACAddr = remoteNode.MACAddr; 
12AA:  MOVLW  01
12AC:  MOVWF  FEA
12AE:  MOVLW  16
12B0:  MOVWF  FE9
12B2:  MOVLW  01
12B4:  MOVWF  FE2
12B6:  MOVLW  7F
12B8:  MOVWF  FE1
12BA:  MOVLW  06
12BC:  MOVWF  01
12BE:  MOVFF  FE6,FEE
12C2:  DECFSZ 01,F
12C4:  BRA    12BE
....................             Cache.IPAddr.Val = remoteNode.IPAddr.Val; 
12C6:  MOVFF  188,11F
12CA:  MOVFF  187,11E
12CE:  MOVFF  186,11D
12D2:  MOVFF  185,11C
....................             break; 
12D6:  MOVLB  0
12D8:  BRA    1302
....................         } 
....................         else 
12DA:  BRA    12E2
12DC:  MOVLB  1
....................             smARP = SM_ARP_REPLY; 
12DE:  BSF    59.1
12E0:  MOVLB  0
....................  
....................     default: 
.................... 		if(ARPPut(&remoteNode, ARP_REPLY)) 
12E2:  MOVLW  01
12E4:  MOVLB  1
12E6:  MOVWF  x8B
12E8:  MOVLW  7F
12EA:  MOVWF  x8A
12EC:  CLRF   x8C
12EE:  MOVLB  0
12F0:  BRA    10E8
12F2:  MOVF   01,F
12F4:  BZ    12FA
.................... 		{ 
.................... 			smARP = SM_ARP_IDLE; 
12F6:  BCF    59.1
.................... 		} 
....................         else 
12F8:  BRA    1300
....................             return FALSE; 
12FA:  MOVLW  00
12FC:  MOVWF  01
12FE:  BRA    1306
....................         break; 
1300:  BRA    1302
....................  
....................     } 
....................     return TRUE; 
1302:  MOVLW  01
1304:  MOVWF  01
.................... } 
1306:  GOTO   3808 (RETURN)
....................  
.................... /********************************************************************* 
....................  * Function:        void ARPResolve(IP_ADDR* IPAddr) 
....................  * 
....................  * PreCondition:    MACIsTxReady(TRUE) returns TRUE 
....................  * 
....................  * Input:           IPAddr  - IP Address to be resolved. 
....................  * 
....................  * Output:          None 
....................  * 
....................  * Side Effects:    None 
....................  * 
....................  * Overview:        An ARP request is sent. 
....................  ********************************************************************/ 
.................... void ARPResolve(IP_ADDR *IPAddr) 
.................... { 
....................     NODE_INFO remoteNode; 
....................  
....................     remoteNode.IPAddr = *IPAddr; 
....................  
....................     ARPPut(&remoteNode, ARP_REQUEST); 
.................... } 
....................  
....................  
....................  
.................... /********************************************************************* 
....................  * Function:        BOOL ARPIsResolved(IP_ADDR* IPAddr, 
....................  *                                      MAC_ADDR *MACAddr) 
....................  * 
....................  * PreCondition:    None 
....................  * 
....................  * Input:           IPAddr      - IPAddress to be resolved. 
....................  *                  MACAddr     - Buffer to hold corresponding 
....................  *                                MAC Address. 
....................  * 
....................  * Output:          TRUE if given IP Address has been resolved. 
....................  *                  FALSE otherwise. 
....................  * 
....................  * Side Effects:    None 
....................  * 
....................  * Overview:        None 
....................  * 
....................  ********************************************************************/ 
.................... BOOL ARPIsResolved(IP_ADDR *IPAddr, MAC_ADDR *MACAddr) 
.................... { 
....................     if(Cache.IPAddr.Val == IPAddr->Val || Cache.IPAddr.Val == AppConfig.MyGateway.Val) 
....................     { 
....................         *MACAddr = Cache.MACAddr; 
....................         return TRUE; 
....................     } 
....................     return FALSE; 
.................... } 
